        <!-- BEGIN PAGE LEVEL PLUGINS -->
        <link href="{{ asset('ressources/vendor/css/form/select2.min.css') }}" rel="stylesheet" type="text/css" />
        <link href="{{ asset('ressources/vendor/css/form/select2-bootstrap.min.css') }}" rel="stylesheet" type="text/css" />
        <!-- END PAGE LEVEL PLUGINS -->
        <!-- BEGIN THEME GLOBAL STYLES -->
        <link href="{{ asset('ressources/vendor/css/components.min.css') }}" rel="stylesheet" id="style_components" type="text/css" />
        <link href="{{ asset('ressources/vendor/css/plugins.min.css') }}" rel="stylesheet" type="text/css" />
        <!-- END THEME GLOBAL STYLES -->
